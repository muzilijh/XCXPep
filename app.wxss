/* 授权膜太层 */
.empower{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99;
}
.empower::after{
  content: '';
  display: block;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.5);
  position: absolute;
  top:0;
  left: 0;
}
.empowerImg{
  width: 297rpx;
  height: 334rpx;
  margin: 159rpx auto 0;
  display: block;
  position: relative;
  z-index: 3;
}
.empowerBody{
  width: 482rpx;
  height: 369rpx;
  background: #ffffff;
  border-radius: 16rpx;
  position: relative;
  top: -70rpx;
  z-index: 2;
  margin: 0 auto;
}
.empbtn{
  width: 300rpx;
  height: 72rpx;
  font-size: 28rpx;
  color:#fff;
  border-radius: 72rpx;
}
.empbtn::after{
  content: none;
}

